{
  "name": "User",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "birthday": {
      "type": "date",
      "required": true
    },
    "gender": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": [
        "object"
      ]
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "goals": {
      "type": "hasMany",
      "model": "Goal",
      "foreignKey": "userId"
    },
    "panels": {
      "type": "hasMany",
      "model": "Panel",
      "foreignKey": "userId"
    },
    "tasks": {
      "type": "hasMany",
      "model": "Task",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
